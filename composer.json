{
    "name": "mdcoderu/illuminate-schedule",
    "type": "project",
    "description": "Use laravel Schedule in non-Laravel applications",
    "keywords": [
        "schedule",
        "cron",
        "manager",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "illuminate/console": "^6",
        "illuminate/events": "^6",
        "illuminate/cache": "^6",
        "illuminate/filesystem": "^6",
        "dragonmantank/cron-expression": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Schedule\\": "src/"
        },
        "files": [
            "helpers.php"
        ]
    },
    "scripts": {
        "lint": "php-cs-fixer fix --diff --dry-run",
        "fix": "php-cs-fixer fix",
        "scan": "psalm --show-info=false"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "vimeo/psalm": "^3.6"
    }
}
